# Baseflow Environment Variables
# Copy to .env and fill in values

# REQUIRED: Your deployed contract address on Base mainnet
NEXT_PUBLIC_CONTRACT_ADDRESS=

# REQUIRED: Your Frame URL (localhost for dev, production URL after deploy)
NEXT_PUBLIC_URL=http://localhost:3000

# OPTIONAL: Base RPC URL (uses public RPC by default)
NEXT_PUBLIC_BASE_RPC_URL=https://mainnet.base.org

# OPTIONAL: WalletConnect Project ID (get one at https://cloud.walletconnect.com)
NEXT_PUBLIC_WALLETCONNECT_ID=
